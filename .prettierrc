{
   "printWidth": 100,
   "tabWidth": 4,
   "useTabs": false,
   "semi": false,
   "singleQuote": true,
   "quoteProps": "consistent",
   "jsxSingleQuote": false,
   "trailingComma": "all",
   "bracketSpacing": true,
   "jsxBracketSameLine": true,
   "arrowParens": "avoid",
   "htmlWhitespaceSensitivity": "strict",
   "endOfLine": "auto",
   "newLine": "lf"
}